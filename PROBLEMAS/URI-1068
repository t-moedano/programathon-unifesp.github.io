#include <iostream>
#include <stack>
#include <string>


using namespace std;

int main()
{
   stack<int> pilha;
   string s1;
   bool b1;
   while(getline(cin,s1)!=NULL) {
        b1=true;
        for(int i = 0; i < s1.length(); i++) {
                if(s1[i] == '(')
                    pilha.push(1);

                else if(s1[i] == ')'){
                    if(pilha.empty()) {
                        cout << "incorrect" << endl;
                        b1 = false;
                        break;
                    }
                    else
                        pilha.pop();
                }
        }

          if(!pilha.empty()) {
            cout << "incorrect" << endl;
             b1 = false;
          }

        while (!pilha.empty())
              pilha.pop();

        if(b1 == true) {
            cout << "correct" << endl;
           // b1 = true;
        }

    }

    return 0;
}
